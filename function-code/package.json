{
  "name": "cron-job-lambda",
  "version": "0.0.1",
  "description": "Template for cron job lambdas written in TypeScript",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf lib && rollup -c",
    "start": "ts-node-dev -r dotenv/config --transpileOnly ./src/index.ts",
    "watch": "ts-node-dev -r dotenv/config --respawn --transpileOnly ./src/index.ts",
    "test": "jest src --coverage"
  },
  "devDependencies": {
    "@types/args": "3.0.0",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.4",
    "@typescript-eslint/eslint-plugin": "2.2.0",
    "@typescript-eslint/parser": "2.2.0",
    "args": "5.0.1",
    "cross-env": "6.0.3",
    "dotenv": "8.1.0",
    "eslint": "6.3.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "23.0.1",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "jest": "24.9.0",
    "jest-each": "24.9.0",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "rollup": "1.27.5",
    "rollup-plugin-typescript2": "0.25.2",
    "ts-jest": "24.0.2",
    "ts-node-dev": "1.0.0-pre.42",
    "typescript": "3.6.2"
  }
}
